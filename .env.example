# Application settings
NODE_ENV=development
PORT=3000

# Database connection
DATABASE_URL=postgres://username:password@localhost:5432/reportanalyser

# Security
SECRET_KEY=your_secret_key_here

# Domain settings (for production)
HOST_DOMAIN=app.yourdomain.com

# Gemini API Key (required for AI document analysis)
GEMINI_API_KEY=your_gemini_api_key_here

# MinIO Configuration (optional, for cloud storage)
MINIO_ENDPOINT=your-minio-server.example.com
MINIO_PORT=443
MINIO_USE_SSL=true
MINIO_ACCESS_KEY=your_minio_access_key
MINIO_SECRET_KEY=your_minio_secret_key
MINIO_BUCKET=generalstorage 